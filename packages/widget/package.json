{
  "name": "@superfluid-finance/widget",
  "version": "0.5.1",
  "private": false,
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "default": "./dist/components/index.js"
    },
    "./webcomponent": {
      "types": "./dist/webcomponent/WebComponent.d.ts",
      "default": "./dist/webcomponent/WebComponent.js"
    },
    "./tokenlist": {
      "types": "./dist/index.tokenlist.d.ts",
      "default": "./dist/index.tokenlist.js"
    },
    "./metadata": {
      "types": "./dist/index.metadata.d.ts",
      "default": "./dist/index.metadata.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "default": "./dist/utils.js"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "components": [
        "./dist/components/index.d.ts"
      ],
      "webcomponent": [
        "./dist/webcomponent/WebComponent.d.ts"
      ],
      "tokenlist": [
        "./dist/index.tokenlist.d.ts"
      ],
      "metadata": [
        "./dist/index.metadata.d.ts"
      ],
      "utils": [
        "./dist/utils.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "cp ../../README.md . && tsc -p ./tsconfig.build.json",
    "dev": "tsc -w -p ./tsconfig.build.json",
    "typedoc": "typedoc --out docs src/index.ts --tsconfig ./tsconfig.build.json",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "wagmi": "wagmi generate",
    "test": "vitest",
    "tsc": "tsc -p ./tsconfig.build.json --noEmit"
  },
  "sideEffects": false,
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@hookform/resolvers": "^3.9.1",
    "@mui/icons-material": "^5.16.11",
    "@mui/lab": "5.0.0-alpha.147",
    "@mui/utils": "^5.16.8",
    "@superfluid-finance/metadata": "^1.5.2",
    "@superfluid-finance/tokenlist": "^5.16.0",
    "@tanstack/react-query": "^5.62.8",
    "abitype": "^0.10.3",
    "blockies-ts": "^1.0.0",
    "immer": "^10.1.1",
    "lodash.isequal": "^4.5.0",
    "lodash.memoize": "^4.1.2",
    "lodash.uniqwith": "^4.5.0",
    "nanoid": "^5.0.9",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.1.2",
    "react-hook-form": "~7.47.0",
    "use-immer": "^0.10.0",
    "zod": "^3.24.1",
    "zod-validation-error": "^3.4.0"
  },
  "devDependencies": {
    "@mui/material": "^5.16.11",
    "@superfluid-finance/ethereum-contracts": "^1.12.0",
    "@testing-library/react": "^14.3.1",
    "@types/lodash.isequal": "^4.5.8",
    "@types/lodash.memoize": "^4.1.9",
    "@types/lodash.uniqwith": "^4.5.9",
    "@types/react": "^18.3.16",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "@wagmi/cli": "^2.1.21",
    "@wagmi/core": "2",
    "jsdom": "^22.1.0",
    "tsconfig": "workspace:*",
    "typedoc": "^0.27.5",
    "typescript": "^5.7.2",
    "viem": "^2.21.55",
    "vitest": "^0.34.6",
    "wagmi": "^2.14.3"
  },
  "peerDependencies": {
    "viem": "^2",
    "wagmi": "^2"
  }
}